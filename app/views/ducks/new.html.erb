<h1>New Ducky</h1>


<% if flash[:my_errors] %>
    <ul>
        <% flash[:my_errors].each do |error|%>
            <li><%= error %></li>
            <% end %>
    </ul>
    <% end %>

<%= form_for @duck do |f| %> <br>
    <%= f.label :name %> <br>
    <%= f.text_field :name %> <br>

    <%= f.label :description %> <br>
    <%= f.text_area :description %> <br>

    <%= f.label :student_id %> <br>
    <%= f.collection_select :student_id, @students, :id, :name  %> <br>

    <%= f.submit %>
<% end %>